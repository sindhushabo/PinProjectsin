<div class="transitions-enabled" id="pins">
  <% @pins.each do |pin| %>
      <div class="box panel panel-default">
        <%= link_to (image_tag pin.image.url), pin %>
        <h2>
          <%= link_to pin.title, pin %>
        </h2>
        <p>
          <%= pin.get_likes.size %> Likes <%= pin.get_dislikes.size %> Dislikes
        </p>
        <p>
          <%= pin.comments.count %> Comments
        </p>
        <p class="user">
          User's email:
          <%= pin.user.email %>
        </p>
      </div>
  <% end %>
</div>
